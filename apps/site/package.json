    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@remix-run/cloudflare": "^1.10.1",
    "@remix-run/cloudflare-pages": "^1.10.1",
    "@remix-run/react": "^1.10.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^8.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@remix-run/dev": "^1.10.1",
