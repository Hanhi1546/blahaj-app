{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build:remix": "remix build --sourcemap",
    "build:routes": "routes-gen -d @routes-gen/remix --post-export \"prettier --write %path\"",
    "build": "run-s build:*",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev:routes": "routes-gen -d @routes-gen/remix --post-export \"prettier --write %path\" -w",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc -b",
    "wrangler": "wrangler pages dev ./public --compatibility-date 2022-11-30 --service=RESVG=blahaj-app-resvg-worker"
  },
